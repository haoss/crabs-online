doctype

html(lang="ru")

  -var active = 'Контакты' //активный пункт меню, список в header.jade
  -var pageTitle = 'Контакты' + ' - Crabs Agency' //title страницы для генерации верстки

  include ./_include/_head

  body#body

    // Preloader
    include ./_include/_preloader

    include ./_include/_header

    #section9.section.section--top.contacts
      .container.section__container
        .section__text.section__text--7 контакты

        .section__header.section__header--big
          .h1 Контакты

        .contacts__wrapper
          .contacts__block
            .contacts__block-title Соцсети
            .contacts__links
              a(href="#!" target="_blank")
                i.icon-facebook
              a(href="#!" target="_blank")
                i.icon-twitter
              a(href="#!" target="_blank")
                i.icon-vk
              a(href="#!" target="_blank")
                i.icon-google-plus
          .contacts__block
            .contacts__block-title Месенджеры
            .contacts__links.contacts__links--add2
              a(href="#!" target="_blank")
                i.icon-brand-telegram
              a(href="#!" target="_blank")
                i.icon-skype
          .contacts__block
            .contacts__block-title Связаться с нами
            a(href="#feedback").btn.open-popup-link Написать


    include ./_include/_footer
    //- div(style="height: 1000px; background-color: #000")

    // Load Scripts
    include ./_include/_footer-js
    script(src="./js/common.js")

